    "version": "0.2.0",
    "configurations": [
        {
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "name": "Purr nodemon",
            "cwd": "${workspaceFolder}/purr",
            "program": "server.js",
            "request": "launch",
            "restart": true,
            "runtimeExecutable": "nodemon",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Purr Node Launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\purr\\server.js"
        },
        
    ]
}